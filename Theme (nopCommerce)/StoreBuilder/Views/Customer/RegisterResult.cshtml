@model RegisterResultModel
@using Nop.Web.Models.Customer;
@{
    Layout = "~/Views/Shared/_ColumnsOne.cshtml";

    //title
    Html.AddTitleParts(T("PageTitle.Register").Text);

    //continue URL
    var qs = this.Context.Request.QueryString["returnUrl"];
    var continueUrl = "";
    if (!String.IsNullOrEmpty(qs) && Url.IsLocalUrl(qs))
    {
        continueUrl = qs;
    }
    else
    {
        continueUrl = Url.RouteUrl("HomePage");
    }
}

<section id="register-result">
    <div class="container">
        <h2 class="wow bounceIn page-title-h2" data-wow-delay="0.4s">@T("Account.Register")</h2>
        <div class="row">
            <div class="col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2">
                <p class="result wow fadeInUp" data-wow-delay="0.6s">@Model.Result</p>
                <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3 wow fadeInUp" data-wow-delay="0.8s">
                    <input type="button" class="btn btn-input" onclick="location.href='@continueUrl'" value="@T("Account.Register.Result.Continue")" />
                </div>
            </div>
        </div>
    </div>
</section>